<div class="entire-panel">
	    
	    {{#each leagues as |league|}}
	    <div id='info-{{league.id}}' class="card">
	        <h5 class="card-header">{{league.name}}</h5>
	        <div class="card-body">
	            <p class="card-text">Information Goes Here</p>
	            <a href="/league/invite/" class="btn btn-primary" id="inviteButton">Invite Players</a>
	        </div>
	    </div>
	    {{/each}}
	    <div id='info-test1' class="card">
	        <h5 class="card-header">Selected name of league from home page goes here</h5>
	        <div class="card-body">
	            <p class="card-text">Information Goes Here</p>
	            <a href="/league/invite/" class="btn btn-primary" id="inviteButton">Invite Players</a>
	        </div>
	    </div>
	</div>
	

